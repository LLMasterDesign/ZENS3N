{
  "meta": {
    "version": "${VERSION}",
    "runtime": "ruby"
  },
  "resources": {
    "files": {
      "max_file_size_mb": 50,
      "max_files_per_operation": 50
    },
    "web_search": {
      "max_results_per_query": 10,
      "max_queries_per_session": 5
    }
  },
  "emergency": {
    "thresholds": {
      "context_utilization_critical": 95,
      "disk_space_critical_mb": 100
    },
    "actions": {
      "halt_on_critical_error": true,
      "auto_backup_on_emergency": true
    }
  }
}

