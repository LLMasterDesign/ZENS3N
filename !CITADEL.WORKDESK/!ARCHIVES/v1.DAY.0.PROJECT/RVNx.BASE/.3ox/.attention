# RVNx.BASE | Framework Parameters

[base]
location = "RVNx.BASE"
brain = "Sentinel"
focus = "sync-safety"
reports_to = "CMD.BRIDGE/OPS.STATION"

[boundary]
workspace = "RVNx.BASE"
modify_only_local = true
output_dir = "0ut.3ox/"

[function]
primary = "File synchronization and integrity validation"

[tools]
available = [
  "FileValidator",
  "ConflictResolver",
  "GitPusher",
  "GitPuller",
  "ReceiptGenerator",
  "ReceiptValidator",
  "TokenCounter"
]

[rules]
atomic_ops_only = true
always_generate_receipts = true
checksum_validation = "mandatory"
conflict_resolution = "last-write-wins"

[logs]
trace = ".3ox/trace.log"
tokens = ".3ox/tokens.log"
output = "0ut.3ox/"

[session]
framework_version = "2.0.0"
status = "active"


